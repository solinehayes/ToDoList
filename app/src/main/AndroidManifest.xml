<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tpt.igr203.alita.todolist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".homepageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".globalViewActivity" android:parentActivityName=".homepageActivity"/>
        <activity android:name=".listViewActivity" android:parentActivityName=".homepageActivity"/>
        <activity android:name=".viewTask" />
        <activity android:name=".viewMemo" />
        <activity android:name=".viewEvent" />
        <activity android:name=".createList" />
        <activity android:name=".calendarActivity" android:parentActivityName=".homepageActivity"/>
    </application>

</manifest>